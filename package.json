{
  "name": "rivet",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --aot",
    "test": "ng test",
    "test-prod": "ng test Rivet --watch=false --browsers=ChromeHeadless --code-coverage",
    "test-package-prod": "ng test rivet-style --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "npm run build_lib_prod && npm run copy_lib_assets && npm run copy_lib_polyfill",
    "build_lib_prod": "ng build --prod rivet-style",
    "copy_lib_assets": "npm run copy_lib_icons && npm run copy_lib_styles",
    "copy_lib_icons": "cpx projects/rivet-style/src/lib/rivet-icons/* dist/rivet-style/lib/rivet-icons/",
    "copy_lib_polyfill": "cpx projects/rivet-style/src/lib/rivet-style.polyfill.js dist/rivet-style/lib",
    "copy_lib_styles": "cpx projects/rivet-style/src/lib/styles/* dist/rivet-style/lib/styles/",
    "link_package": "npm run package && cd dist/rivet-style && npm link",
    "unlink_package": "cd dist/rivet-style && npm unlink",
    "npm_pack": "cd dist/rivet-style && npm pack",
    "npm_publish": "cd dist/rivet-style && npm publish",
    "package": "npm run build_lib && npm run npm_pack",
    "build_icons": "icon-font-generator projects/rivet-style/src/lib/rivet-icons/svgs/*.svg -o projects/rivet-style/src/lib/rivet-icons --csspath projects/rivet-style/src/lib/styles/iconStyles.scss --csstp projects/rivet-style/src/lib/rivet-icons/icon-style-template.hbs --html false --cssfontsurl #{$rivet-icon-path} -n rivet-icons -t * --height 1000 -j false -p rivet-icon"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.8",
    "@angular/cdk": "^9.0.0-rc.6",
    "@angular/common": "^9.0.0-rc.8",
    "@angular/compiler": "^9.0.0-rc.8",
    "@angular/core": "^9.0.0-rc.8",
    "@angular/forms": "^9.0.0-rc.8",
    "@angular/material": "^9.0.0-rc.6",
    "@angular/platform-browser": "^9.0.0-rc.8",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.8",
    "@angular/router": "^9.0.0-rc.8",
    "accounting": "^0.4.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.6.2",
    "css-vars-ponyfill": "^2.1.2",
    "hammerjs": "^2.0.8",
    "rivet-style": "latest",
    "rxjs": "^6.5.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.8",
    "@angular-devkit/build-ng-packagr": "~0.900.0-rc.8",
    "@angular/cli": "^9.0.0-rc.8",
    "@angular/compiler-cli": "^9.0.0-rc.8",
    "@angular/language-service": "^9.0.0-rc.8",
    "@types/accounting": "^0.4.1",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.1.4",
    "codelyzer": "^5.2.1",
    "cpx": "^1.5.0",
    "glob": "^7.1.6",
    "icon-font-generator": "^2.1.10",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "ng-packagr": "^9.0.0-rc.2",
    "protractor": "^5.4.2",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.6.4",
    "webfonts-generator": "^0.4.0"
  }
}
