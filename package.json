{
  "name": "rivet",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build rivet-style --prod",
    "test": "ng test",
    "lint": "ng lint",
    "test-package-prod": "ng test rivet-style --watch=false --browsers=ChromeHeadless --code-coverage",
    "build_lib": "npm run build_lib_prod && npm run copy_lib_assets",
    "build_lib_prod": "ng build --prod rivet-style",
    "copy_lib_assets": "npm run copy_lib_icons && npm run copy_lib_styles",
    "copy_lib_icons": "cpx projects/rivet-style/src/lib/rivet-icons/* dist/rivet-style/lib/rivet-icons/",
    "copy_lib_styles": "cpx projects/rivet-style/src/lib/styles/* dist/rivet-style/lib/styles/",
    "npm_pack": "cd dist/rivet-style && npm pack",
    "npm_publish": "cd dist/rivet-style && npm publish",
    "package": "npm run build_lib && npm run npm_pack",
    "link": "npm run package && cd dist/rivet-style && npm link",
    "build_icons": "icon-font-generator projects/rivet-style/src/lib/rivet-icons/svgs/*.svg -o projects/rivet-style/src/lib/rivet-icons --csspath projects/rivet-style/src/lib/styles/iconStyles.scss --csstp projects/rivet-style/src/lib/rivet-icons/icon-style-template.hbs --html false --cssfontsurl #{$rivet-icon-path} -n rivet-icons -t * --height 1000 -j false -p rivet-icon"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.4",
    "@angular/cdk": "^10.1.0",
    "@angular/common": "^10.0.4",
    "@angular/compiler": "^10.0.4",
    "@angular/core": "^10.0.4",
    "@angular/forms": "^10.0.4",
    "@angular/material": "^10.1.0",
    "@angular/platform-browser": "^10.0.4",
    "@angular/platform-browser-dynamic": "^10.0.4",
    "accounting": "^0.4.1",
    "css-vars-ponyfill": "^2.1.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular-devkit/build-ng-packagr": "~0.1000.3",
    "@angular/cli": "^10.0.3",
    "@angular/compiler-cli": "^10.0.4",
    "@angular/language-service": "^10.0.4",
    "@types/accounting": "^0.4.1",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.1.4",
    "codelyzer": "^6.0.0",
    "cpx": "^1.5.0",
    "glob": "^7.1.6",
    "icon-font-generator": "^2.1.10",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.5.4",
    "tslint": "~6.1.0",
    "typescript": "^3.9.7",
    "webfonts-generator": "^0.4.0"
  }
}
