{
  "name": "rivet",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --aot",
    "test": "ng test",
    "test-prod": "ng test Rivet --watch=false --browsers=ChromeHeadless --code-coverage",
    "test-package-prod": "ng test rivet-style --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "npm run build_lib_prod && npm run copy_lib_assets",
    "build_lib_prod": "ng build --prod rivet-style",
    "copy_lib_assets": "npm run copy_lib_icons && npm run copy_lib_styles",
    "copy_lib_icons": "cpx projects/rivet-style/src/lib/rivet-icons/* dist/rivet-style/lib/rivet-icons/",
    "copy_lib_styles": "cpx projects/rivet-style/src/lib/styles/* dist/rivet-style/lib/styles/",
    "link_package": "npm run package && cd dist/rivet-style && npm link",
    "unlink_package": "cd dist/rivet-style && npm unlink",
    "npm_pack": "cd dist/rivet-style && npm pack",
    "npm_publish": "cd dist/rivet-style && npm publish",
    "package": "npm run build_lib && npm run npm_pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.7",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/forms": "^7.2.7",
    "@angular/http": "^7.2.7",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/router": "^7.2.7",
    "accounting": "^0.4.1",
    "core-js": "^2.6.5",
    "rivet-style": "file:dist/rivet-style",
    "rxjs": "^6.4.0",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.3",
    "@angular-devkit/build-ng-packagr": "^0.13.3",
    "@angular/cli": "^7.3.3",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "^7.2.7",
    "@types/accounting": "^0.4.1",
    "@types/jasmine": "^3.3.9",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^11.9.5",
    "codelyzer": "^4.5.0",
    "cpx": "^1.5.0",
    "glob": "^7.1.3",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.7.1",
    "protractor": "^5.4.2",
    "ts-node": "^8.0.2",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.0",
    "tslint": "^5.13.0",
    "typescript": "^3.2.4"
  }
}
